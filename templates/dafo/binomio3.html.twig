{% extends 'base.html.twig' %}

{% block title %}Ejemplo DAFO{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    table,td,tr,th {
        border: 1px solid black;
    }
</style>
<div class="example-wrapper">
<form method="post" action="{{ path('binomio3') }}">
    {% for binomio in binomios %}
     {{  binomio.getQuestionInterna().getTipusDafo().getTipus()}} {{binomio.getQuestionInterna().getNombre() }} x {{  binomio.getQuestionExterna().getTipusDafo().getTipus()}} {{ binomio.getQuestionExterna().getNombre() }} <input type="checkbox" name="{{binomio.id}}" value="{{binomio.id}}"><br /> 
    {% endfor %}

    <input type="submit" value="Enviar"/>
</form>
</div>
{% endblock %}
